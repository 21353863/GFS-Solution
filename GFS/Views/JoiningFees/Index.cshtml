@model IEnumerable<GFS.Models.JoiningFee>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.policyNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CustomerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AmountRendered)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.change)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cashierName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.branch)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.policyNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CustomerName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AmountRendered)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.change)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cashierName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.branch)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.refNo }) |
            @Html.ActionLink("Details", "Details", new { id=item.refNo }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.refNo })
        </td>
    </tr>
}

</table>
